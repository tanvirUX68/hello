@font-face{
	src: url(../font/SaintCarell.otf);
	font-family: SaintCarell;
}


body{
	
}
.heading{
	text-align: center;
	color: #00ADB5;
	text-shadow: 0px 1px 2px black;
	font-family: SaintCarell;
	font-size: 48px;
	

}
.article{
	text-align: justify;
	line-height: 1.8;
	text-indent: 200px;
	
	
}
.bold{
	color: red;
	font-weight: 600;
	font-size: 20px;
}
.link{
	text-decoration: none;
	text-transform: lowercase;
	text-align: center;
}
.name{
	color: #13005A;
	font-size: 20px;
	fonr-weight: 600;
	list-style: none;
}
.image{
	align: right;
	color: #13005A;
	list-style: square outside url(../images/icon.png);
	padding-left: 50px;
}

